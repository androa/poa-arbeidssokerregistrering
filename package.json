{
    "name": "poa-arbeidssokerregistrering",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "npm run lint && jest --watch",
        "test:ci": "npm run lint && jest --passWithNoTests",
        "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.css'",
        "refresh": "rm -rf node_modules && rm package-lock.json && npm i"
    },
    "dependencies": {
        "@navikt/ds-css": "0.15.10",
        "@navikt/ds-react": "0.16.16",
        "@navikt/nav-dekoratoren-moduler": "1.6.9",
        "next": "12.1.0",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.2",
        "@testing-library/react": "12.1.3",
        "@types/jest": "27.4.1",
        "@types/node": "17.0.21",
        "@types/react": "17.0.39",
        "eslint": "8.10.0",
        "eslint-config-next": "12.1.0",
        "husky": "7.0.4",
        "jest": "27.5.1",
        "lint-staged": "12.3.4",
        "msw": "0.38.1",
        "prettier": "2.5.1",
        "typescript": "4.5.5"
    },
    "engines": {
        "node": ">=16.14.0"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
    }
